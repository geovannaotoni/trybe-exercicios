function soma(a, b) {
  return a + b;
}

function subtracao(a, b) {
  return a -b;
}

function multiplicacao(a, b){
  return a * b;
}

function divisao(a, b) {
  return a / b;
}

function modulo(a,b) {
  return a % b;
}

console.log(soma(5,2));
console.log(subtracao(5,2));
console.log(multiplicacao(5,2));
console.log(divisao(5,2));
console.log(modulo(5,2));